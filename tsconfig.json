{
  "compilerOptions": {
    "lib": [
      "es2020","esnext",
      "dom"
    ],
    "module": "CommonJS",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "types": [
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./types",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "build/types/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "src/assets",
    "node_modules",
    "build",
    "cache",
    "artifacts"
  ]
}
